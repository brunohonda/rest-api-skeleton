{
  "name": "rest-api-skeleton",
  "version": "0.0.1",
  "description": "Skeleton application to Rest API with ExpressJS and Typescript",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "ts-node-dev --respawn --transpileOnly --ignore-watch node_modules --no-notify src/server.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunohonda/resp-api-skeleton.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "typescript"
  ],
  "author": "Bruno Honda <bruno.honda@live.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunohonda/resp-api-skeleton/issues"
  },
  "homepage": "https://github.com/brunohonda/resp-api-skeleton#readme",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-typescript": "^7.10.1",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.3.0",
    "jest": "^26.0.1",
    "ts-jest": "^26.1.0",
    "ts-node-dev": "^1.0.0-pre.49",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  }
}
