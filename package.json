{
  "name": "rest-api-skeleton",
  "version": "0.0.2",
  "description": "Skeleton application to Rest API with ExpressJS and Typescript",
  "main": "server.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch node_modules --no-notify src/server.ts",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brunohonda/resp-api-skeleton.git"
  },
  "keywords": [
    "nodejs",
    "expressjs",
    "typescript"
  ],
  "author": "Bruno Honda <bruno.honda@live.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brunohonda/resp-api-skeleton/issues"
  },
  "homepage": "https://github.com/brunohonda/resp-api-skeleton#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.5",
    "@babel/node": "^7.14.5",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "babel-plugin-module-resolver": "^4.0.0",
    "eslint": "^7.28.0",
    "jest": "^27.0.4",
    "ts-jest": "^27.0.3",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.3.2"
  }
}
